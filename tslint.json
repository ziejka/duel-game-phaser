{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": false,
        "interface-name": [
            true,
            "never-prefix"
        ],
        "jsx-no-multiline-js": false,
        "member-access": false,
        "no-empty": false,
        "object-literal-key-quotes": [
            true,
            "as-needed"
        ],
        "max-line-length": false,
        "object-literal-sort-keys": false,
        "quotemark": false,
        "semicolon": [
            true,
            "never"
        ],
        "trailing-comma": false,
        "one-variable-per-declaration": false,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ]
    },
    "rulesDirectory": []
}